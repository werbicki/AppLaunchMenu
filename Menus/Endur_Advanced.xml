<AppLaunchMenu SecurityGroup="Everyone">
	<ScriptListing>
		<Script Name="Endur_Config" Language="C#">
			<![CDATA[
WriteLine($"# Endur configuration file");

foreach (var objVariable in Environment)			
	WriteLine($"set {objVariable.Name} = {objVariable.ExpandedValue}");
			]]>
		</Script>
		<Script Name="Endur_Spread" Language="C#">
			<![CDATA[
WriteLine($@"# Spread configuration file

# Global Options

# (Deprecated) MaxSessionMessages = 3000
# (Deprecated) MaxSessionsAllowed = 600
OnlyLeaveMemberships = True
EventLogFile = {Environment["OLF_ENVIRONMENT_PATH"]}\\Logs\\spread\\spread_%h.log
EventTimeStamp = ""[%a %d %b %Y %H:%M:%S]""

; Segment-level Options

Spread_Segment 127.0.0.1:{Environment["OLF_ECOM_PORT"]} {{
	{Environment["AB_ECOM_EXTERNAL_DAEMON"]}	192.168.1.1
}}
Spread_Segment 127.0.0.1:30803 {{
	{Environment["AB_ECOM_EXTERNAL_DAEMON1"]}	192.168.2.1
}}
");
			]]>
		</Script>
		<Script Name="Endur_STunnel" Language="C#">
			<![CDATA[
WriteLine($@"; STunnel configuration file

; Global Options

; Use it for client mode
client = yes
sslVersionMin = TLSv1.2
sslVersionMax = TLSv1.2
options = NO_SSLv2
options = NO_SSLv3

; Service-level Options

[STunnel_Endur_{Environment["OLF_ENVIRONMENT"]}]
accept = {Environment("AB_ICE_GATEWAY_STUNNEL_PORT")}
connect = fixtc-a1.ice.com:443
; 158.224.35.15
TIMEOUTclose = 0

; Debugging (may be useful for troubleshooting)
debug = 7
output = {Environment["OLF_ENVIRONMENT_PATH"]}\Logs\stunnel\stunnel_1.log
");
			]]>
		</Script>
		<Script Name="GetCurrentDate" Language="C#">
			<![CDATA[
HttpClient objHttpClient = new HttpClient();
string strDateTime = DateTime.Today.ToString("yyyy-MM-dd");

try
{
    string url = "http://worldtimeapi.org/api/timezone/Etc/UTC";
    HttpResponseMessage objHttpResponseMessage = await objHttpClient.GetAsync(url);

    objHttpResponseMessage.EnsureSuccessStatusCode();

    string objRsponseBody = await objHttpResponseMessage.Content.ReadAsStringAsync();

    // Deserialize only the datetime field
    using JsonDocument objJsonDocument = JsonDocument.Parse(objRsponseBody);
    JsonElement objJsonElementRoot = objJsonDocument.RootElement;
    strDateTime = DateTime.Parse(objJsonElementRoot.GetProperty("datetime").GetString()!).ToString("yyyy-MM-dd");
}
catch (HttpRequestException e)
{
    Exception(e.Message);
}

return strDateTime;
			]]>
		</Script>
